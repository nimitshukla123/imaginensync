<defaul>
   <mtfastion>
      <header>
         <sticky_enable>1</sticky_enable>
         <hide_on_mobile>0</hide_on_mobile>
         <compare>1</compare>
         <layout>layout2</layout>
      </header>
      <menu>
         <effect>slide</effect>
         <menuleft_effect>fade</menuleft_effect>
         <home>1</home>
         <enable_custom>1</enable_custom>
         <static_identifier>custom_menu</static_identifier>
         <show_if_no_children>0</show_if_no_children>
         <label1>New</label1>
         <label2>Hot!</label2>
         <url_key>contact:left;about-magento-demo-store:left</url_key>
         <static_identifier>custom_menu:right</static_identifier>
      </menu>
      <category>
         <aspect_ratio>0</aspect_ratio>
         <alt_image>0</alt_image>
         <alt_image_column>position</alt_image_column>
         <alt_image_column_value>2</alt_image_column_value>
         <image_width>500</image_width>
         <image_height>611</image_height>
      </category>
      <category_grid>
         <column_count>3</column_count>
         <column_count_768>3</column_count_768>
         <column_count_640>2</column_count_640>
         <column_count_480>2</column_count_480>
         <equal_height>1</equal_height>
         <hover_effect>0</hover_effect>
         <display_rating>2</display_rating>
         <display_addtocart>2</display_addtocart>
         <display_addtolinks>2</display_addtolinks>
         <addtolinks_simple>2</addtolinks_simple>
      </category_grid>
      <category_list>
         <hover_effect>1</hover_effect>
         <addtolinks_simple>1</addtolinks_simple>
      </category_list>
      <product_page>
         <replace_upsell>1</replace_upsell>
         <zoom_enable>1</zoom_enable>
         <zoom_width>350</zoom_width>
         <zoom_height>350</zoom_height>
         <zoom_position>inside</zoom_position>
         <zoom_count>3</zoom_count>
         <zoom_margin>5</zoom_margin>
         <upsell_hover_slider>1</upsell_hover_slider>
         <replace_related>2</replace_related>
         <related_hover_slider>1</related_hover_slider>
         <layout>default</layout>
         <product_social>1</product_social>
      </product_page>
      <product_labels>
         <new>new</new>
         <sale>sale</sale>
      </product_labels>
      <footer>
         <links_column_auto_width>1</links_column_auto_width>
      </footer>
      <product_slider>
         <speed>400</speed>
         <move_items>0</move_items>
      </product_slider>
      <magento_blocks>
         <top_links>1</top_links>
         <footer_newsletter>1</footer_newsletter>
         <related_products_checkbox>0</related_products_checkbox>
      </magento_blocks>
      <sidebar_blocks_home>
         <compare>0</compare>
         <compared>0</compared>
         <viewed>0</viewed>
         <wishlist>0</wishlist>
         <poll>0</poll>
         <tags>0</tags>
         <paypal>0</paypal>
      </sidebar_blocks_home>
      <install>
         <overwrite_blocks>0</overwrite_blocks>
         <overwrite_pages>0</overwrite_pages>
      </install>
      <customization>
         <custom_css>0</custom_css>
      </customization>
   </mtfastion>
   <mtfastion_design>
      <colors>
         <primary_color>#fb006d</primary_color>
         <text_color>#aaaaaa</text_color>
         <link_color>#313233</link_color>
         <link_hover_color>#fb006d</link_hover_color>
         <button_bg_color>#000000</button_bg_color>
         <button_color>#ffffff</button_color>
         <button_hover_color>#ffffff</button_hover_color>
         <button_bg_hover_color>#fb006d</button_bg_hover_color>
         <background_icon_color>rgba(0, 0, 0, 0)</background_icon_color>
         <icon_color>#aaaaaa</icon_color>
         <hover_background_icon_color>rgba(0, 0, 0, 0)</hover_background_icon_color>
         <hover_icon_color>#282828</hover_icon_color>
         <price_color>#282828</price_color>
         <heading_color_title>#282828</heading_color_title>
      </colors>
      <font>
         <font_size>13px</font_size>
         <primary_font_family_group>google</primary_font_family_group>
         <primary_font_family_custom>&apos;Lato&apos;, sans-serif</primary_font_family_custom>
         <primary_font_family>Lato</primary_font_family>
         <primary_char_latin_ext>0</primary_char_latin_ext>
         <heading_font_family_custom>&apos;Playfair Display&apos;, sans-serif</heading_font_family_custom>
      </font>
      <page>
         <bg_color>#ffffff</bg_color>
         <bg_repeat>repeat</bg_repeat>
         <bg_attachment>scroll</bg_attachment>
         <bg_positionx>center</bg_positionx>
         <bg_positiony>top</bg_positiony>
         <patterns>none</patterns>
      </page>
      <header>
         <bg_top_color>transparent</bg_top_color>
         <top_color>#000000</top_color>
         <top_link_color>#000000</top_link_color>
         <top_link_hover_color>#fb006d</top_link_hover_color>
         <patterns>none</patterns>
         <inner_bg_color>transparent</inner_bg_color>
         <color>#333333</color>
         <link_color>#333333</link_color>
         <link_hover_color>#333333</link_hover_color>
      </header>
      <nav>
         <inner_bg_color>rgba(0, 0, 0, 0)</inner_bg_color>
         <font_size>15</font_size>
         <font_uppercase>1</font_uppercase>
         <bg_color>rgba(0, 0, 0, 0)</bg_color>
         <color>#ffffff</color>
         <hover_bg_color>rgba(0, 0, 0, 0)</hover_bg_color>
         <hover_color>#fb006d</hover_color>
         <submenu_bg_color>#2d2d2d</submenu_bg_color>
         <submenu_color>#858585</submenu_color>
         <submenu_hover_color>#fb006d</submenu_hover_color>
         <canvas_title_color>#fff</canvas_title_color>
         <canvas_bg_content_color>#000000</canvas_bg_content_color>
         <canvas_bg_color>#000000</canvas_bg_color>
         <canvas_color>#ccc</canvas_color>
         <canvas_hover_bg_color>#000000</canvas_hover_bg_color>
         <canvas_hover_color>#fff</canvas_hover_color>
         <vertical_bg_title_color>#cfb985</vertical_bg_title_color>
         <vertical_title_color>#343434</vertical_title_color>
         <vertical_link_color>#3b3b3b</vertical_link_color>
         <vertical_link_hover_color>#d02a21</vertical_link_hover_color>
      </nav>
      <main>
         <bg_color>#ffffff</bg_color>
         <patterns>none</patterns>
      </main>
      <product_page>
         <addto_cart_bg_color>#000000</addto_cart_bg_color>
         <addto_cart_color>#ffffff</addto_cart_color>
         <addto_cart_hover_bg_color>#fb006d</addto_cart_hover_bg_color>
         <addto_cart_hover_color>#ffffff</addto_cart_hover_color>
         <tab_inner_bg_color>#ffffff</tab_inner_bg_color>
         <tab_color>#3b3b3b</tab_color>
         <tab_hover_bg_color>#ffffff</tab_hover_bg_color>
         <tab_hover_color>#ee2d54</tab_hover_color>
         <tab_active_bg_color>#ffffff</tab_active_bg_color>
         <tab_active_color>#ee2d54</tab_active_color>
         <tab_panel_bg_color>#ffffff</tab_panel_bg_color>
         <tab_text_color>#272727</tab_text_color>
      </product_page>
      <footer>
         <bg_color>#292929</bg_color>
         <patterns>none</patterns>
      </footer>
      <footer_top>
         <bg_color>#1a1a1a</bg_color>
         <link_color>#aaaaaa</link_color>
         <color>#aaaaaa</color>
         <hover_color>#fb006d</hover_color>
      </footer_top>
      <footer_bottom>
         <bg_color>#1a1a1a</bg_color>
         <link_color>#ffffff</link_color>
         <color>#aaaaaa</color>
         <hover_color>#fb006d</hover_color>
      </footer_bottom>
      <footer_copyright>
         <bg_color>#000000</bg_color>
         <link_active_color>#ffffff</link_active_color>
         <color>#aaaaaa</color>
      </footer_copyright>
   </mtfastion_design>
   <mtfastion_layout>
      <responsive>
         <responsive>1</responsive>
         <rtl_language>0</rtl_language>
         <fluid_width>0</fluid_width>
      </responsive>
   </mtfastion_layout>
   <mtattribute>
      <general>
         <show>0</show>
         <width>0</width>
      </general>
   </mtattribute>
   <ajaxcart>
      <ajaxcart_config>
         <enable_ajaxcart>1</enable_ajaxcart>
         <enable_ajaxcompare>1</enable_ajaxcompare>
         <enable_ajaxwishlist>1</enable_ajaxwishlist>
         <enable_quickview>1</enable_quickview>
      </ajaxcart_config>
   </ajaxcart>
   <mtfilter>
      <general>
         <bar>1</bar>
         <bar_color>CC181E</bar_color>
      </general>
      <catalog>
         <price>1</price>
         <enable>1</enable>
         <main_selector>.col-main</main_selector>
         <layer_selector>.block-layered-nav</layer_selector>
      </catalog>
      <catalogsearch>
         <price>1</price>
         <enable>1</enable>
         <main_selector>.col-main</main_selector>
         <layer_selector>.block-layered-nav</layer_selector>
      </catalogsearch>
   </mtfilter>
   <mtnewsletter>
      <display_options>
         <enable>1</enable>
         <background_color>#ffffff</background_color>
         <background_image>default/newsletter.jpg</background_image>
         <intro>Subscribe to the MT Fastion eCommerce newsletter to receive timely updates from your favorite products.</intro>
         <width>700</width>
         <height>400</height>
      </display_options>
   </mtnewsletter>
</defaul>
